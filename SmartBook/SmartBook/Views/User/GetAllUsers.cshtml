@model IEnumerable<SmartBook.Models.ApplicationUser>


@{
    ViewBag.Title = "User List";
}

<h2>
    All Users
</h2>


<table class="table">
    <tr>
        <th>
            @Html.ActionLink("Username", "GetAllUsers")
        </th>
        <th>
            @Html.ActionLink("Email", "GetAllUsers")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.UserName, "Edit", new { id = item.Id })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
